{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/src/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "C:/msys64/mingw64/bin/gcc.exe",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "windows-gcc-x64",
            "configurationProvider": "ms-vscode.makefile-tools",
            "compilerArgs": [
                "-Wall",
                "-fPIC",
                "-DSM64_LIB_EXPORT",
                "-DVERSION_US",
                "-DDEBUG_LEVEL_ROOMS",
                "-DNO_SEGMENTED_MEMORY",
                "-DGBI_FLOATS",
                "-Wno-unknown-pragmas",
                "-lm",
                "-shared",
                "-lpthread"
            ]
        }
    ],
    "version": 4
}